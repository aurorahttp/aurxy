{
    "name": "panlatent/aurxy",
    "description": "HTTP proxy server with powerful customizable filter rules",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "ext-ev": "*",
        "guzzlehttp/guzzle": "~6.3",
        "symfony/console": "~3.4",
        "symfony/dom-crawler": "~3.4",
        "symfony/css-selector": "~3.4",
        "symfony/event-dispatcher": "~3.4",
        "symfony/yaml": "~3.4",
        "symfony/options-resolver": "~3.4",
        "symfony/process": "~3.4",
        "monolog/monolog": "^1.23",
        "panlatent/http-foundation": "~1.0",
        "panlatent/http-exception": "~1.0",
        "panlatent/http-message": "~1.0",
        "panlatent/http-message-codec": "~1.0",
        "panlatent/http-transaction": "~1.0"
    },
    "require-dev": {
        "phing/phing": "^2.16",
        "codeception/codeception": "^2.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "panlatent",
            "email": "panlatent@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Panlatent\\Aurxy\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
